<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.mcafee.scor.safety.rawData.RawData" table="rawData">
      <id name="id" type="int" column="id">
         <generator class="native"/>
      </id>
      <property name="coordinates" column="coordinates" type="string"/>
      <property name="numberOfCrimes" column="number_of_crimes" type="int"/>
      <property name="timeOfDay" column="time_of_day">
      	<type name="org.hibernate.type.EnumType">
      		<param name="enumClass">com.mcafee.scor.safety.model.TimeOfDay</param>
      	</type>
      </property>
   </class>
</hibernate-mapping>